{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Urls": "http://localhost:8088",
	"SuppliersRemote": "http://localhost:5911",
	"spring": {
		"application": {
			"name": "products_microservice"
		}
	},
	"eureka": {
		"client": {
			"serviceUrl": "http://localhost:8761/eureka",
